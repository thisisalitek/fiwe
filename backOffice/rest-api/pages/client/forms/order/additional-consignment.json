{
	"form": [{
		"type": "form",
		"fields": {
			"orderType": { "type": "hidden", "dataType": "string", "value": "additional-consignment" },
			"orderKey": { "text": "Sipariş Anahtarı", "type": "string", "col": 4, "required": true },
			"orderDate": { "text": "Tarih", "type": "date", "col": 3, "required": true },
			"orderLine": {
				"text": "Satırlar",
				"type": "grid",
				"fields": {
					"itemDescription": { "text": "Malzeme", "type": "string", "html":"${itemDescription}<br><b>Kod:${itemName}</b> UBB:${ubb} SUT:${sut}" },
					"itemName": { "text": "Malzeme Kodu", "type": "string", "readonly": false },
					"ubb": { "text": "UBB", "type": "string", "readonly": false },
					"sut": { "text": "SUT", "type": "string", "readonly": false },
					"quantity": { "text": "Miktar", "type": "quantity", "round": 0 }
				},
				"options": {
					"selection": false,
					"filter": false,
					"confirmBeforeRemove": true,
					"buttons": {
						"add": true,
						"edit": true,
						"delete": true,
						"view": false,
						"print": false,
						"copy": false
					}
				}
			}
		},
		"dataSource": { "method": "GET", "url": "/dbapi/order" },
		"options":{
			"returnUrl":"/haham#/order/order-list"
		}
	}]
}